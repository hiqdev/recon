{
    "name": "hiqdev/recon",
    "type": "library",
    "require": {
        "php": ">=7.2.0",

        "yiisoft/yii2": "3.0.x-dev",
        "hiqdev/yii-compat": "dev-master",

        "hiqdev/hiapi": "dev-master",
        "hiqdev/yii2-autobus": "dev-master",
        "hiqdev/yii2-data-mapper": "dev-master"
    },
    "require-dev": {
        "hiqdev/hidev-php": "dev-master",
        "hiqdev/hidev-hiqdev": "dev-master",
        "hiqdev/hidev": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "hiqdev\\recon\\core\\": "src",
            "hiqdev\\recon\\core\\tests\\": "tests"
        }
    },
    "extra": {
        "config-plugin": {
            "console": "config/console.php",
            "common": "config/common.php",
            "params": "config/params.php"
        }
    },
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packagist.hiqdev.com"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
